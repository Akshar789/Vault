{
  "version": 2,
  "name": "vault-backend",
  "builds": [
    {
      "src": "apps/backend/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/backend/src/main.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
